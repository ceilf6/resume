{
  "name": "@ceilf6/resume-components",
  "version": "0.1.4",
  "description": "美观、优雅、精炼的简历组件库",
  "keywords": [
    "resume",
    "components",
    "react",
    "typescript"
  ],
  "author": "ceilf6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceilf6/resume.git",
    "directory": "component"
  },
  "homepage": "https://github.com/ceilf6/resume/tree/main/component#readme",
  "bugs": {
    "url": "https://github.com/ceilf6/resume/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "glob": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}